local quality
quality=adj:box("shaders quality", "value", function(a)
	local c=tonumber(a)
	if c>10 then
		if c>21 then
			quality:text("21")
			sett1['Rendering']['QualityLevel']='Level21'
		else
			sett1['Rendering']['QualityLevel']='Level'..c
		end
	elseif c<10 then
		sett1['Rendering']['QualityLevel']='Level0'..c
	end
end)

